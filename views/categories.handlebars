{{! post card }}
<div class="row">
    <div>
      <h2 class="text-center mt-5 mb-5">You're searching for <span class="text-success">{{categoryName}}</span></h2>
    </div>
</div>

{{#each guitarsPosts as |guitarPost|}}
  <div class="d-flex justify-content-center">
  <section>
    <div class='container w-75 card mb-3 mt-3'>
      <h3 class='card-header text-success mt-2 d-flex justify-content-center'>Instrument for sale!</h3>
      <div class='card-body pb-0'>
        <h5 class='card-title'><a
            href='/posts/{{id}}'
            class='post-link text-dark-emphasis fs-2 d-flex justify-content-center'
          >{{title}}</a>
        </h5>
      <div class="card-price mb-3 d-flex justify-content-center">
        <span class='badge mt-2 bg-success w-25 py-3 fs-6'>${{price}}</span>
      </div>
      </div>
      <div>
      {{#if image}}
        <img src='{{image}}' class='card-img-top' alt='Product Image' />
      {{else}}
        <svg
          xmlns='http://www.w3.org/2000/svg'
          class='d-block user-select-none ms-4'
          width='100%'
          height='100%'
          aria-label='Placeholder: Image cap'
          focusable='false'
          role='img'
          preserveAspectRatio='xMidYMid slice'
          viewBox='0 0 318 180'
          style='font-size:1.125rem;text-anchor:middle'
        >
          <rect
            y='18'
            rx='8'
            ry='8'
            width='80%'
            height='80%'
            fill='#868e96'
          ></rect>
          <text
            x='40%'
            y='50%'
            text-anchor='middle'
            fill='#dee2e6'
            dy='.3em'
          >Product img</text>
        </svg>
      {{/if}}
      </div>
      <div class="d-flex mt-4 mb-3 border-top border-bottom px-2 py-2">
        <p class='card-subtitle text-muted'>{{content}}</p>
      </div>
      <div class='card-options mb-2 d-flex justify-content-center'>
        <a href='#' class='card-link text-info text-decoration-none'>Buy now</a>
        <a href='#' class='card-link text-info text-decoration-none'>Comment</a>
      </div>
      <div class='card-footer text-body-tertiary pb-2 d-flex justify-content-center'>
        Posted by
        {{user.name}}
        on
        {{format_date createdAt}}
        at
        {{format_time createdAt}}
      </div>
    </div>
  </section>
  </div>
{{/each}}

